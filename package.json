{
    "name": "@democrance/lint-config",
    "type": "module",
    "version": "0.10.1",
    "packageManager": "pnpm@10.9.0",
    "author": "Daniil Chumachenko <daniil4udo@gmail.com> (https://github.com/daniil4udo/)",
    "license": "MIT",
    "scripts": {
        "prepare": "pnpm -r run stub",
        "inspect": "eslint --print-config ./.eslintrc.cjs > .eslintrc.json",
        "lint": "eslint . --color --fix",
        "release": "bumpp packages/*/package.json --commit 'release: ðŸ”– v' --tag --no-push && pnpm -r publish --access public",
        "test": "pnpm -r run test"
    },
    "devDependencies": {
        "@democrance/eslint-config": "workspace:*",
        "bumpp": "^10.1.0",
        "eslint": "^9.25.1",
        "typescript": "^5.8.3"
    }
}
